{
    "name": "metolabs/camelot-php",
    "description": "Camelot wrapper for PHP",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MetoLabs\\CamelotPHP\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gerardo Ramirez",
            "email": "gera.96@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/process": "^6.0 || ^7.0",
        "spatie/temporary-directory": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit"
    },
    "suggest": {
        "camelot-py": "Required to perform PDF table extraction. See https://camelot-py.readthedocs.io/en/master/user/install.html"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}